@{
    ViewData["Title"] = "Thank You";
}

<h2>@ViewData["Title"]</h2>

<div class="alert alert-success mt-6">
    <strong>Thank you for your enquiry!</strong>
    <p>Your message has been successfully submitted. We will get back to you as soon as possible.</p>
</div>
<div class="d-flex justify-content-between mt-3">
    <a asp-controller="Account" asp-action="Login" class="text-center">Go to Login</a>
    <a asp-controller="Contact" asp-action="Index" class="text-center">Return to Contact Page</a>
</div>
<a href="@Url.Action("Index", "Contact")" class="btn btn-primary">Return to Contact Page</a>
